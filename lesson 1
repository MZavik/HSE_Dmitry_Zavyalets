# Создаем переменные
name = "Иван"
age = 25

# Выводим значения переменных на экран
print(f"Имя: {name}, Возраст: {age}")

# Запрос информации у пользователя (в реальном исполнении, не в этой среде)
user_name = input("Введите ваше имя: ")
user_age = input("Введите ваш возраст: ")

# Выводим введенные данные
print(f"Ваше имя: {user_name}, ваш возраст: {user_age}")

# Использование функции id() для демонстрации адреса в памяти
print(f"Адрес переменной name в памяти: {id(name)}")

